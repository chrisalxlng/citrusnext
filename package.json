{
  "name": "mantine-next-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cd backend; npm run serve_db_detached; npm run serve_backend_detached; cd ..; npm run serve_dev_frontend",
    "prod": "cd backend; npm run serve_db_detached; npm run serve_backend_detached; cd ..; npm run build; npm run start",
    "serve_dev_frontend": "next dev",
    "serve_db": "cd backend; docker-compose up",
    "serve_db_detached": "cd backend; docker-compose up -d",
    "serve_backend": "cd backend; mvn spring-boot:run",
    "serve_backend_detached": "cd backend; mvn spring-boot:start",
    "build": "next build",
    "analyze": "ANALYZE=true next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "export": "next build && next export",
    "lint": "eslint --ext .ts,.tsx"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint"
    }
  },
  "dependencies": {
    "@mantine/core": "4.2.8",
    "@mantine/dates": "4.2.8",
    "@mantine/hooks": "4.2.8",
    "@mantine/next": "4.2.8",
    "@mantine/notifications": "4.2.8",
    "@mantine/prism": "4.2.8",
    "@mantine/spotlight": "^4.2.8",
    "axios": "^0.27.2",
    "cookies-next": "^2.0.4",
    "dayjs": "^1.10.8",
    "next": "12.1.0",
    "next-i18next": "^11.0.0",
    "prism-react-renderer": "^1.3.1",
    "react": "17.0.2",
    "react-cookie": "^4.1.1",
    "react-dom": "17.0.2",
    "react-jwt": "^1.1.6",
    "react-query": "^3.39.0",
    "react-window": "^1.8.7",
    "tabler-icons-react": "^1.39.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.1.0",
    "@next/eslint-plugin-next": "^12.1.0",
    "@types/node": "^17.0.21",
    "@types/react": "17.0.40",
    "@types/react-window": "^1.8.5",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.1",
    "eslint-config-mantine": "1.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "prettier": "^2.6.0",
    "pretty-quick": "^3.1.3",
    "typescript": "4.6.2"
  }
}
